[flake8]
max-line-length = 120
extend-ignore = E203,W503
exclude =
    .git,
    __pycache__,
    archive,
    archives,
    build,
    builds,
    deployment/deployment_package_*/*,
    deployment_package_*,
    scripts/enterprise/*,
    scripts/optimization/*,
    scripts/utilities/*,
    copilot_qiskit_stubs/*,
    db_tools/*,
    enterprise_modules/*,
    quantum/*,
    quantum_optimizer.py,
    databases/*.db,
    *.log,
    scripts/enterprise/*,
    scripts/optimization/*,
    scripts/utilities/*,
    copilot_qiskit_stubs/*,
    db_tools/*,
    enterprise_modules/*,
    quantum/*,
    quantum_optimizer.py
