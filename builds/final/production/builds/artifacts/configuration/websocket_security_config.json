{
  "host": "localhost",
  "port": 8765,
  "ssl_enabled": true,
  "ssl_cert_path": "certs/server.crt",
  "ssl_key_path": "certs/server.key",
  "max_connections": 100,
  "connection_timeout": 30,
  "ping_interval": 20,
  "ping_timeout": 10,
  "compression": "deflate",
  "security": {
    "require_auth": true,
    "rate_limit": {
      "enabled": true,
      "max_messages_per_minute": 60
    },
    "cors": {
      "enabled": true,
      "origins": [
        "https://localhost:5000",
        "https://127.0.0.1:5000"
      ]
    }
  }
}